    "@ionic/core": "^8.5.2"
  },
  "devDependencies": {
    "typescript": "^5.0.0",
    "undici": "^5.0.0"
  },
  "bugs": {
    "url": "https://github.com/ionic-sveltekit/core/issues"
  },
  "homepage": "https://github.com/ionic-sveltekit/core#readme",
  "keywords": [
