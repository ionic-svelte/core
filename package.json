{
  "name": "@ionic-svelte/core",
  "version": "0.6.1",
  "private": false,
  "homepage": "https://ionic-svelte.firebaseapp.com",
  "repository": {
    "type": "git",
    "url": "https://github.com/ionic-svelte/core.git"
  },
  "keywords": [
    "ui library",
    "ui",
