{
  "name": "@ionic-sveltekit/core",
  "description": "Ionic UI components with SvelteKit",
  "version": "0.0.9",
  "private": false,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ionic-sveltekit/core.git"
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
